  "scripts": {
    "build": "nest build",
    "dev": "nest start --watch",
    "start": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix & nest build --type-check",
    "migrations:generate": "npm run build && npx mikro-orm migration:create",
    "migrations:run": "npm run build && npx mikro-orm migration:up",
    "migrations:check": "mikro-orm migration:check",
    "seed": "mikro-orm seeder:run",
    "initialize": "rimraf data/packet-run.db && npm run migrations:run && npm run seed && redis-cli KEYS \"bull:default*\" | xargs redis-cli DEL"
  },
  "dependencies": {
    "@apollo/server": "^4.12.1",
