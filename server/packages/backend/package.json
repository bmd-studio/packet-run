    "lodash": "^4.17.21",
    "nanoid": "^3.3.11",
    "nodejs-traceroute": "^2.0.1",
    "puppeteer": "^24.9.0",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^6.0.1",
    "rxjs": "^8.0.0-alpha.14"
  },
